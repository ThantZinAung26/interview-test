<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:noNamespaceSchemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
	<display-name>Thant-Sin-Aung</display-name>
	<welcome-file-list>
		<welcome-file>list-Invoice.xhtml</welcome-file>
	</welcome-file-list>
        
         <context-param>
            <param-name>primefaces.THEME</param-name>
            <param-value>bootstrap</param-value>
        </context-param>
	
	<servlet>
		<servlet-name>FacesServlet</servlet-name>
		<display-name>FacesServlet</display-name>
		<servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FacesServlet</servlet-name>
		<url-pattern>*.xhtml</url-pattern>
	</servlet-mapping>
</web-app>